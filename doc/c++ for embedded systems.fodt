   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T13"><text:s text:c="4"/></text:span><text:span text:style-name="T27">size_t</text:span><text:span text:style-name="T13"> tail = CyclicBufferIncrement(cyclicBuffer-&gt;</text:span><text:span text:style-name="T43">tail</text:span><text:span text:style-name="T13">, CYCLIC_BUFFRE_SIZE);</text:span></text:p>
   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T13"><text:s text:c="4"/></text:span><text:span text:style-name="T5">bool</text:span><text:span text:style-name="T13"> res = (cyclicBuffer-&gt;</text:span><text:span text:style-name="T43">head</text:span><text:span text:style-name="T13"> == tail);</text:span></text:p>
   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T13"><text:s text:c="4"/></text:span><text:span text:style-name="T5">return</text:span><text:span text:style-name="T13"> res;</text:span></text:p>
   <text:p text:style-name="P17">}</text:p>
