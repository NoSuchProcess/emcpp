   <text:p text:style-name="P46"/>
   <text:p text:style-name="P48">Finally a usage example:</text:p>
   <text:p text:style-name="P37"><text:span text:style-name="T7">static</text:span><text:span text:style-name="T58"> </text:span><text:span text:style-name="T7">void</text:span><text:span text:style-name="T58"> </text:span><text:span text:style-name="T59">mainExpirationHandler</text:span><text:span text:style-name="T58">(</text:span><text:span text:style-name="T7">const</text:span><text:span text:style-name="T58"> </text:span><text:span text:style-name="T30">Timer</text:span><text:span text:style-name="T58">&amp; timer) {</text:span></text:p>
   <text:p text:style-name="P1"><text:span text:style-name="T16"><text:s text:c="4"/></text:span><text:span text:style-name="T31">TimerID</text:span><text:span text:style-name="T16"> timerId = timer.getId();</text:span></text:p>
   <text:p text:style-name="P1"><text:span text:style-name="T16"><text:s text:c="4"/></text:span><text:span text:style-name="T31">uintptr_t</text:span><text:span text:style-name="T16"> data = timer.getApplicationData();</text:span></text:p>
   <text:p text:style-name="P1"><text:span text:style-name="T16"><text:s text:c="4"/>cout &lt;&lt; </text:span><text:span text:style-name="T38">&quot;Expired id=&quot;</text:span><text:span text:style-name="T16"> &lt;&lt; timerId &lt;&lt; </text:span><text:span text:style-name="T38">&quot;,</text:span><text:span text:style-name="T39">appdata</text:span><text:span text:style-name="T38">=&quot;</text:span><text:span text:style-name="T16"> &lt;&lt; data &lt;&lt; </text:span><text:span text:style-name="T43">endl</text:span><text:span text:style-name="T16">;</text:span></text:p>
   <text:p text:style-name="P1"><text:span text:style-name="T16">}</text:span></text:p>
