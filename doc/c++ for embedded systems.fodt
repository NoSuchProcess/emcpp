   <text:p text:style-name="Preformatted_20_Text"/>
   <text:p text:style-name="P53"><text:span text:style-name="T5">enum</text:span><text:span text:style-name="T16"> TimerError </text:span><text:span text:style-name="T18">TimerList::startTimer</text:span><text:span text:style-name="T16">(</text:span><text:span text:style-name="T37">SystemTime</text:span><text:span text:style-name="T16"> currentTime,</text:span></text:p>
