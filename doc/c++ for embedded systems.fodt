   <text:p text:style-name="Preformatted_20_Text"/>
   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T16"><text:s text:c="4"/></text:span><text:span text:style-name="T37">Timer</text:span><text:span text:style-name="T16">* newTimer;</text:span></text:p>
   <text:p text:style-name="Preformatted_20_Text"/>
   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T16"><text:s text:c="4"/></text:span><text:span text:style-name="T5">if</text:span><text:span text:style-name="T16"> (</text:span><text:span text:style-name="T57">freeTimers</text:span><text:span text:style-name="T16">.isEmpty())</text:span></text:p>
