   <text:p text:style-name="P24"><text:span text:style-name="T5">void</text:span><text:span text:style-name="T13"> </text:span><text:span text:style-name="T15">Timer::setApplicationData</text:span><text:span text:style-name="T13">(</text:span><text:span text:style-name="T27">uintptr_t</text:span><text:span text:style-name="T13"> applicationData) {</text:span></text:p>
   <text:p text:style-name="P24"><text:span text:style-name="T13"><text:s text:c="4"/></text:span><text:span text:style-name="T5">this</text:span><text:span text:style-name="T13">-&gt;</text:span><text:span text:style-name="T43">applicationData</text:span><text:span text:style-name="T13"> = applicationData;</text:span></text:p>
