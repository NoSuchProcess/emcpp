   <text:p text:style-name="P46"><text:span text:style-name="T5">void</text:span><text:span text:style-name="T13"> <text:s/></text:span><text:span text:style-name="T15">Timer::start</text:span><text:span text:style-name="T13">() {</text:span></text:p>
   <text:p text:style-name="P24"><text:span text:style-name="T13"><text:s text:c="4"/></text:span><text:span text:style-name="T43">running</text:span><text:span text:style-name="T13"> = </text:span><text:span text:style-name="T5">true</text:span><text:span text:style-name="T13">;</text:span></text:p>
