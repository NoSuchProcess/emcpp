   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T16"><text:s text:c="4"/></text:span><text:span text:style-name="T5">return</text:span><text:span text:style-name="T16"> res;</text:span></text:p>
   <text:p text:style-name="Preformatted_20_Text">}</text:p>
   <text:p text:style-name="Text_20_body"/>
   <text:p text:style-name="P44"><text:span text:style-name="T75">The protected m</text:span>ethod _startTimer() moves a timer from the list of <text:span text:style-name="T75">the </text:span>free timers to the list of <text:span text:style-name="T75">the </text:span>running timers</text:p>
   <text:p text:style-name="P28"><text:span text:style-name="T5">enum</text:span><text:span text:style-name="T16"> TimerError </text:span><text:span text:style-name="T18">TimerList::_startTimer</text:span><text:span text:style-name="T16">(</text:span></text:p>
   <text:p text:style-name="P28"><text:span text:style-name="T16"><text:s text:c="3"/></text:span><text:span text:style-name="T37">SystemTime</text:span><text:span text:style-name="T16"> currentTime,</text:span></text:p>
   <text:p text:style-name="P28"><text:span text:style-name="T16"><text:s text:c="3"/></text:span><text:span text:style-name="T37">SystemTime </text:span><text:span text:style-name="T16">&amp;nearestExpirationTime, </text:span></text:p>
