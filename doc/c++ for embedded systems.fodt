   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T16"><text:s text:c="4"/></text:span><text:span text:style-name="T31">size_t</text:span><text:span text:style-name="T16"> </text:span><text:span text:style-name="T17">getSize</text:span><text:span text:style-name="T16">() </text:span><text:span text:style-name="T6">const</text:span><text:span text:style-name="T16"> {</text:span></text:p>
   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T16"><text:s text:c="8"/></text:span><text:span text:style-name="T6">return</text:span><text:span text:style-name="T16"> </text:span><text:span text:style-name="T53">size</text:span><text:span text:style-name="T16">;</text:span></text:p>
