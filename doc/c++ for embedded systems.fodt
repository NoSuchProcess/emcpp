   <text:p text:style-name="P24"><text:span text:style-name="T13"><text:s text:c="4"/></text:span><text:span text:style-name="T5">static_assert</text:span><text:span text:style-name="T13">(</text:span><text:span text:style-name="T5">sizeof</text:span><text:span text:style-name="T13">(</text:span><text:span text:style-name="T36">ObjectType</text:span><text:span text:style-name="T13">) &lt;= </text:span><text:span text:style-name="T5">sizeof</text:span><text:span text:style-name="T13">(</text:span><text:span text:style-name="T27">uintptr_t</text:span><text:span text:style-name="T13">), </text:span><text:span text:style-name="T32">&quot;CyclicBuffer is intended to work only with integer types or pointers&quot;</text:span><text:span text:style-name="T13">);</text:span></text:p>
