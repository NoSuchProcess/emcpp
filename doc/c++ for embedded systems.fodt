   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T16"><text:s text:c="8"/>cout &lt;&lt; (</text:span><text:span text:style-name="T5">int</text:span><text:span text:style-name="T16">) val &lt;&lt; </text:span><text:span text:style-name="T48">endl</text:span><text:span text:style-name="T16">;</text:span></text:p>
   <text:p text:style-name="P19"><text:s text:c="4"/>};</text:p>
   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T16"><text:s text:c="4"/></text:span><text:span text:style-name="T5">return</text:span><text:span text:style-name="T16"> 0;</text:span></text:p>
   <text:p text:style-name="P19">}</text:p>
