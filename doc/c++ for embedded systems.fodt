   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T5">static</text:span><text:span text:style-name="T13"> </text:span><text:span text:style-name="T5">inline</text:span><text:span text:style-name="T13"> </text:span><text:span text:style-name="T5">void</text:span><text:span text:style-name="T13"> </text:span><text:span text:style-name="T15">errorUnderflow</text:span><text:span text:style-name="T13">() {</text:span></text:p>
