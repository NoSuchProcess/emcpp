   <text:p text:style-name="P1"><text:span text:style-name="T16"><text:s text:c="4"/></text:span><text:span text:style-name="T31">size_t</text:span><text:span text:style-name="T16"> </text:span><text:span text:style-name="T53">tail</text:span><text:span text:style-name="T16">;</text:span></text:p>
   <text:p text:style-name="P1"><text:span text:style-name="T16"><text:s text:c="4"/></text:span><text:span text:style-name="T31">size_t</text:span><text:span text:style-name="T16"> </text:span><text:span text:style-name="T53">size</text:span><text:span text:style-name="T16">;</text:span></text:p>
   <text:p text:style-name="P1"><text:span text:style-name="T16">};</text:span></text:p>
   <text:p text:style-name="Text_20_body"><text:soft-page-break/>Constructor calls new[] to allocate array of pointers:</text:p>
   <text:p text:style-name="P1"><text:span text:style-name="T6">template</text:span><text:span text:style-name="T16">&lt;</text:span><text:span text:style-name="T6">typename</text:span><text:span text:style-name="T16"> </text:span><text:span text:style-name="T45">ObjectType</text:span><text:span text:style-name="T16">, </text:span><text:span text:style-name="T6">typename</text:span><text:span text:style-name="T16"> </text:span><text:span text:style-name="T45">Lock</text:span><text:span text:style-name="T16">&gt; </text:span><text:span text:style-name="T6">inline</text:span><text:span text:style-name="T16"> </text:span><text:span text:style-name="T17">CyclicBufferDynamic&lt;</text:span></text:p>
   <text:p text:style-name="P1"><text:span text:style-name="T17"><text:s text:c="8"/>ObjectType, Lock&gt;::CyclicBufferDynamic</text:span><text:span text:style-name="T16">(</text:span><text:span text:style-name="T31">size_t</text:span><text:span text:style-name="T16"> size) {</text:span></text:p>
