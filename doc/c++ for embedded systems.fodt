   <text:p text:style-name="P27">Example of implementation of the class Stack push and pop methods:</text:p>
   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T6">templa</text:span><text:span text:style-name="T45">te&lt;typenam</text:span><text:span text:style-name="T6">e</text:span><text:span text:style-name="T16"> </text:span><text:span text:style-name="T53">Obje</text:span><text:span text:style-name="T16">c</text:span><text:span text:style-name="T45">tTyp</text:span><text:span text:style-name="T16">e, </text:span><text:span text:style-name="T6">typename</text:span><text:span text:style-name="T16"> Lock, std::size_t Size&gt;</text:span></text:p>
   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T6">inline</text:span><text:span text:style-name="T16"> </text:span><text:span text:style-name="T6">bool</text:span><text:span text:style-name="T16"> Stack&lt;ObjectType, Lock, Size&gt;::push(ObjectType* object) {</text:span></text:p>
   <text:p text:style-name="P14"><text:s text:c="4"/>Lock();</text:p>
   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T16"><text:s text:c="4"/></text:span><text:span text:style-name="T6">if</text:span><text:span text:style-name="T16"> (!isFull()) {</text:span></text:p>
   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T16"><text:s text:c="8"/>data[</text:span><text:span text:style-name="T6">this</text:span><text:span text:style-name="T16">-&gt;top] = object;</text:span></text:p>
   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T16"><text:s text:c="8"/></text:span><text:span text:style-name="T6">this</text:span><text:span text:style-name="T16">-&gt;top++;</text:span></text:p>
   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T16"><text:s text:c="8"/></text:span><text:span text:style-name="T6">return</text:span><text:span text:style-name="T16"> </text:span><text:span text:style-name="T6">true</text:span><text:span text:style-name="T16">;</text:span></text:p>
   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T16"><text:s text:c="4"/>} </text:span><text:span text:style-name="T6">else</text:span><text:span text:style-name="T16"> {</text:span></text:p>
   <text:p text:style-name="P14"><text:s text:c="8"/>errorOverflow();</text:p>
   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T16"><text:s text:c="8"/></text:span><text:span text:style-name="T6">return</text:span><text:span text:style-name="T16"> </text:span><text:span text:style-name="T6">false</text:span><text:span text:style-name="T16">;</text:span></text:p>
   <text:p text:style-name="P14"><text:s text:c="4"/>}</text:p>
   <text:p text:style-name="Preformatted_20_Text"/>
   <text:p text:style-name="P14">}</text:p>
   <text:p text:style-name="Preformatted_20_Text"/>
   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T6">template</text:span><text:span text:style-name="T16">&lt;</text:span><text:span text:style-name="T6">typename</text:span><text:span text:style-name="T16"> ObjectType, </text:span><text:span text:style-name="T6">typename</text:span><text:span text:style-name="T16"> Lock, std::size_t Size&gt;</text:span></text:p>
   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T6">inline</text:span><text:span text:style-name="T16"> </text:span><text:span text:style-name="T6">bool</text:span><text:span text:style-name="T16"> Stack&lt;ObjectType, Lock, Size&gt;::pop(ObjectType** object) {</text:span></text:p>
   <text:p text:style-name="P14"><text:s text:c="4"/>Lock();</text:p>
   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T16"><text:s text:c="4"/></text:span><text:span text:style-name="T6">if</text:span><text:span text:style-name="T16"> (!isEmpty()) {</text:span></text:p>
   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T16"><text:s text:c="8"/></text:span><text:span text:style-name="T6">this</text:span><text:span text:style-name="T16">-&gt;top--;</text:span></text:p>
   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T16"><text:s text:c="8"/>*object = (data[</text:span><text:span text:style-name="T6">this</text:span><text:span text:style-name="T16">-&gt;top]);</text:span></text:p>
   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T16"><text:s text:c="8"/></text:span><text:span text:style-name="T6">return</text:span><text:span text:style-name="T16"> </text:span><text:span text:style-name="T6">true</text:span><text:span text:style-name="T16">;</text:span></text:p>
   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T16"><text:s text:c="4"/>} </text:span><text:span text:style-name="T6">else</text:span><text:span text:style-name="T16"> {</text:span></text:p>
   <text:p text:style-name="P14"><text:s text:c="8"/>errorUnderflow();</text:p>
