   <text:p text:style-name="P1"><text:span text:style-name="T16"><text:s text:c="12"/>cout &lt;&lt; </text:span><text:span text:style-name="T38">&quot;timer start failed for timer &quot;</text:span><text:span text:style-name="T16"> &lt;&lt; i &lt;&lt; </text:span><text:span text:style-name="T43">endl</text:span><text:span text:style-name="T16">;</text:span></text:p>
