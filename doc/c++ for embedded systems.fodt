   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T16"><text:s text:c="4"/></text:span><text:span text:style-name="T37">size_t</text:span><text:span text:style-name="T16"> </text:span><text:span text:style-name="T57">head</text:span><text:span text:style-name="T16">;</text:span></text:p>
   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T16"><text:s text:c="4"/></text:span><text:span text:style-name="T37">size_t</text:span><text:span text:style-name="T16"> </text:span><text:span text:style-name="T57">tail</text:span><text:span text:style-name="T16">;</text:span></text:p>
   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T16"><text:s text:c="4"/></text:span><text:span text:style-name="T37">size_t</text:span><text:span text:style-name="T16"> </text:span><text:span text:style-name="T57">size</text:span><text:span text:style-name="T16">;</text:span></text:p>
