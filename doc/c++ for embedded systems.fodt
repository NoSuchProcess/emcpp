   <text:p text:style-name="P1"><text:span text:style-name="T16"><text:s text:c="4"/>}</text:span></text:p>
   <text:p text:style-name="P3"/>
   <text:p text:style-name="P1"><text:span text:style-name="T16"><text:s text:c="4"/></text:span><text:span text:style-name="T31">size_t</text:span><text:span text:style-name="T16"> </text:span><text:span text:style-name="T17">increment</text:span><text:span text:style-name="T16">(</text:span><text:span text:style-name="T31">size_t</text:span><text:span text:style-name="T16"> index);</text:span></text:p>
