   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T16"><text:s text:c="4"/></text:span><text:span text:style-name="T5">inline</text:span><text:span text:style-name="T16"> </text:span><text:span text:style-name="T5">static</text:span><text:span text:style-name="T16"> </text:span><text:span text:style-name="T37">TimerID</text:span><text:span text:style-name="T16"> </text:span><text:span text:style-name="T18">getNextId</text:span><text:span text:style-name="T16">();</text:span></text:p>
