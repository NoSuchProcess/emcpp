   <text:p text:style-name="Preformatted_20_Text"/>
   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T16"><text:s text:c="4"/></text:span><text:span text:style-name="T37">TimerExpirationHandler</text:span><text:span text:style-name="T16"> </text:span><text:span text:style-name="T57">expirationHandler</text:span><text:span text:style-name="T16">;</text:span></text:p>
