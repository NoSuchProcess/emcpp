   <text:p text:style-name="P17"><text:s text:c="4"/>}</text:p>
   <text:p text:style-name="Preformatted_20_Text"/>
   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T13"><text:s text:c="4"/></text:span><text:span text:style-name="T32">size_t</text:span><text:span text:style-name="T13"> </text:span><text:span text:style-name="T15">getSize</text:span><text:span text:style-name="T13">() </text:span><text:span text:style-name="T4">const</text:span><text:span text:style-name="T13"> {</text:span></text:p>
