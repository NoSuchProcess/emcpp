   <text:p text:style-name="P17"><text:s text:c="4"/>Lock();</text:p>
   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T13"><text:s text:c="4"/></text:span><text:span text:style-name="T4">if</text:span><text:span text:style-name="T13"> (!isEmpty()) {</text:span></text:p>
   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T13"><text:s text:c="8"/></text:span><text:span text:style-name="T4">this</text:span><text:span text:style-name="T13">-&gt;top--;</text:span></text:p>
