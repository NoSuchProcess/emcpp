   <text:p text:style-name="P1"><text:span text:style-name="T4">protected</text:span><text:span text:style-name="T13">:</text:span></text:p>
   <text:p text:style-name="P1"><text:span text:style-name="T13"><text:s text:c="4"/></text:span><text:span text:style-name="T4">const</text:span><text:span text:style-name="T13"> </text:span><text:span text:style-name="T4">char</text:span><text:span text:style-name="T13"> *</text:span><text:span text:style-name="T52">name</text:span><text:span text:style-name="T13">;</text:span></text:p>
   <text:p text:style-name="P1"><text:span text:style-name="T13"><text:s text:c="4"/></text:span><text:span text:style-name="T32">array</text:span><text:span text:style-name="T13">&lt;</text:span><text:span text:style-name="T32">TimerListBase</text:span><text:span text:style-name="T13">*, </text:span><text:span text:style-name="T44">Size</text:span><text:span text:style-name="T13">&gt; </text:span><text:span text:style-name="T52">timerLists</text:span><text:span text:style-name="T13">;</text:span></text:p>
   <text:p text:style-name="P1"><text:span text:style-name="T13"><text:s text:c="4"/></text:span><text:span text:style-name="T32">size_t</text:span><text:span text:style-name="T13"> </text:span><text:span text:style-name="T52">listCount</text:span><text:span text:style-name="T13">;</text:span></text:p>
   <text:p text:style-name="P9">};</text:p>
   <text:p text:style-name="P59">Add list API fills the std:array timerLists:</text:p>
   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T4">template</text:span><text:span text:style-name="T13">&lt;</text:span><text:span text:style-name="T32">size_t</text:span><text:span text:style-name="T13"> </text:span><text:span text:style-name="T44">Size</text:span><text:span text:style-name="T13">&gt; </text:span><text:span text:style-name="T4">inline</text:span><text:span text:style-name="T13"> </text:span><text:span text:style-name="T4">bool</text:span><text:span text:style-name="T13"> </text:span><text:span text:style-name="T15">TimerSet&lt;Size&gt;::addList</text:span><text:span text:style-name="T13">(</text:span><text:span text:style-name="T32">TimerListBase</text:span><text:span text:style-name="T13">* list) {</text:span></text:p>
