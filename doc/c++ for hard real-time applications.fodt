   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T13"><text:s text:c="4"/></text:span><text:span text:style-name="T4">if</text:span><text:span text:style-name="T13"> (timer != </text:span><text:span text:style-name="T4">nullptr</text:span><text:span text:style-name="T13">)</text:span></text:p>
