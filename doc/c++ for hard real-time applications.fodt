   <text:p text:style-name="P6"/>
   <text:p text:style-name="P1"><text:span text:style-name="T13"><text:s text:c="4"/>TimerError </text:span><text:span text:style-name="T15">processExpiredTimers</text:span><text:span text:style-name="T13">(</text:span><text:span text:style-name="T32">SystemTime</text:span><text:span text:style-name="T13"> currentTime,</text:span></text:p>
   <text:p text:style-name="P1"><text:span text:style-name="T13"><text:s text:c="12"/></text:span><text:span text:style-name="T32">SystemTime</text:span><text:span text:style-name="T13">&amp; expirationTime);</text:span></text:p>
   <text:p text:style-name="P6"/>
   <text:p text:style-name="P1"><text:span text:style-name="T13"><text:s text:c="4"/></text:span><text:span text:style-name="T4">inline</text:span><text:span text:style-name="T13"> </text:span><text:span text:style-name="T4">bool</text:span><text:span text:style-name="T13"> </text:span><text:span text:style-name="T15">addList</text:span><text:span text:style-name="T13">(</text:span><text:span text:style-name="T32">TimerListBase</text:span><text:span text:style-name="T13">* list);</text:span></text:p>
